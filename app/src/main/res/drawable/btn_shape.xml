<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Fondo de la forma -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFCC00"/> <!-- Color de fondo -->
            <corners android:radius="10dp"/> <!-- Bordes redondeados -->
        </shape>
    </item>

    <!-- Palma de la pata -->
    <item
        android:left="20dp"
        android:top="10dp"
        android:right="20dp"
        android:bottom="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFCC00"/>
            <corners android:radius="40dp"/>
        </shape>
    </item>

    <!-- Dedos de la pata -->
    <item
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="80dp">
        <shape android:shape="oval">
            <solid android:color="#FFCC00"/>
        </shape>
    </item>

    <item
        android:left="90dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="80dp">
        <shape android:shape="oval">
            <solid android:color="#FFCC00"/>
        </shape>
    </item>

    <item
        android:left="10dp"
        android:top="80dp"
        android:right="90dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FFCC00"/>
        </shape>
    </item>

    <item
        android:left="50dp"
        android:top="80dp"
        android:right="50dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FFCC00"/>
        </shape>
    </item>

</layer-list>